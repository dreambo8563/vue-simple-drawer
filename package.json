{
  "name": "vue-simple-drawer",
  "version": "0.1.0",
  "author": "dreambo8563",
  "main": "dist/vue-simple-drawer.min.js",
  "private": false,
  "description": "A tiny Drawer component for vue",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target wc --name vue-simple-drawer src/components/Drawer/index.vue",
    "test": "vue-cli-service build --target wc --name vue-simple-drawer src/components/Drawer/index.vue",
    "pages": "vue-cli-service build --dest docs",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "report": "vue-cli-service build --report",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "cz-conventional-changelog": "^2.1.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-plugin-unit-jest": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreambo8563/vue-simple-drawer.git"
  },
  "bugs": {
    "url": "https://github.com/dreambo8563/vue-simple-drawer/issues"
  },
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*",
    "package.json",
    "*.js",
    "README.md"
  ],
  "keywords": [
    "vue",
    "drawer",
    "component",
    "panel",
    "layout"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
